    <script>
        const conferences = [
            { acronym: "AAAI", name: "National Conference of the American Association for Artificial Intelligence", rank: "A+" },
            { acronym: "AAMAS", name: "International Conference on Autonomous Agents and Multiagent Systems", rank: "A+" },
            { acronym: "ACL", name: "Association of Computational Linguistics", rank: "A+" },
            { acronym: "ACMMM", name: "ACM Multimedia Conference", rank: "A+" },
            { acronym: "ASPLOS", name: "Architectural Support for Programming Languages and Operating Systems", rank: "A+" },
            { acronym: "CAV", name: "Computer Aided Verification", rank: "A+" },
            { acronym: "CCS", name: "ACM Conference on Computer and Communications Security", rank: "A+" },
            { acronym: "CHI", name: "International Conference on Human Factors in Computing Systems", rank: "A+" },
            { acronym: "COLT", name: "Annual Conference on Computational Learning Theory", rank: "A+" },
            { acronym: "CRYPTO", name: "Advances in Cryptology", rank: "A+" },
            { acronym: "CSCL", name: "Computer Supported Collaborative Learning", rank: "B" },
            { acronym: "DCC", name: "IEEE Data Compression Conference", rank: "A+" },
            { acronym: "DSN", name: "International Conference on Dependable Systems", rank: "A" },
            { acronym: "EuroCrypt", name: "International Conference on the Theory and Application of Cryptographic Techniques", rank: "A+" },
            { acronym: "FOCS", name: "IEEE Symposium on Foundations of Computer Science", rank: "A+" },
            { acronym: "FOGA", name: "Foundations of Genetic Algorithms", rank: "A" },
            { acronym: "HPCA", name: "IEEE Symposium on High Performance Computer Architecture", rank: "A+" },
            { acronym: "I3DG", name: "ACM-SIGRAPH Interactive 3D Graphics", rank: "B" },
            { acronym: "ICAPS", name: "International Conference on Automated Planning and Scheduling", rank: "A+" },
            { acronym: "ICCV", name: "IEEE International Conference on Computer Vision", rank: "A+" },
            { acronym: "ICDE", name: "IEEE International Conference on Data Engineering", rank: "A+" },
            { acronym: "ICDM", name: "IEEE International Conference on Data Mining", rank: "A+" },
            { acronym: "ICFP", name: "International Conference on Functional Programming", rank: "A" },
            { acronym: "ICIS", name: "International Conference on Information Systems", rank: "A" },
            { acronym: "ICML", name: "International Conference on Machine Learning", rank: "A+" },
            { acronym: "ICSE", name: "International Conference on Software Engineering", rank: "A+" },
            { acronym: "IJCAI", name: "International Joint Conference on Artificial Intelligence", rank: "A+" },
            { acronym: "IJCAR", name: "International Joint Conference on Automated Reasoning", rank: "A" },
            { acronym: "INFOCOM", name: "Joint Conference of the IEEE Computer and Communications Societies", rank: "A+" },
            { acronym: "InfoVis", name: "IEEE Information Visualization Conference", rank: "A+" },
            { acronym: "IPSN", name: "Information Processing in Sensor Networks", rank: "A+" },
            { acronym: "ISCA", name: "ACM International Symposium on Computer Architecture", rank: "A+" },
            { acronym: "ISMAR", name: "IEEE and ACM International Symposium on Mixed and Augmented Reality", rank: "A+" },
            { acronym: "ISSAC", name: "International Symposium on Symbolic and Algebraic Computation", rank: "A+" },
            { acronym: "ISWC", name: "IEEE International Symposium on Wearable Computing", rank: "A+" },
            { acronym: "IWQoS", name: "IFIP International Workshop on QoS", rank: "A+" },
            { acronym: "JCDL", name: "ACM Conference on Digital Libraries", rank: "A+" },
            { acronym: "KR", name: "International Conference on Principles of KR & Reasoning", rank: "A+" },
            { acronym: "LICS", name: "IEEE Symposium on Logic in Computer Science", rank: "A+" },
            { acronym: "MOBICOM", name: "ACM International Conference on Mobile Computing and Networking", rank: "A+" },
            { acronym: "NIPS", name: "Advances in Neural Information Processing Systems", rank: "A+" },
            { acronym: "OOPSLA", name: "ACM Conference on Object Oriented Programming Systems Languages and Applications", rank: "A" },
            { acronym: "OSDI", name: "Usenix Symposium on Operating Systems Design and Implementation", rank: "A+" },
            { acronym: "PERCOM", name: "IEEE International Conference on Pervasive Computing and Communications", rank: "A+" },
            { acronym: "PERVASIVE", name: "International Conference on Pervasive Computing", rank: "A+" },
            { acronym: "PLDI", name: "ACM-SIGPLAN Conference on Programming Language Design & Implementation", rank: "A+" },
            { acronym: "PODC", name: "ACM Symposium on Principles of Distributed Computing", rank: "A+" },
            { acronym: "PODS", name: "ACM SIGMOD-SIGACT-SIGART Conference on Principles of Database Systems", rank: "A+" },
            { acronym: "POPL", name: "ACM-SIGACT Symposium on Principles of Programming Languages", rank: "A+" },
            { acronym: "RSS", name: "Robotics: Systems and Science", rank: "A+" },
            { acronym: "RTSS", name: "Real Time Systems Symposium", rank: "A+" },
            { acronym: "SENSYS", name: "ACM Conference on Embedded Networked Sensor Systems", rank: "A+" },
            { acronym: "SIGCOMM", name: "ACM Conference on Applications, Technologies, Architectures, and Protocols for Computer Communication", rank: "A+" },
            { acronym: "SIGGRAPH", name: "ACM SIG International Conference on Computer Graphics and Interactive Techniques", rank: "A+" },
            { acronym: "SIGIR", name: "ACM International Conference on Research and Development in Information Retrieval", rank: "A+" },
            { acronym: "SIGKDD", name: "ACM International Conference on Knowledge Discovery and Data Mining", rank: "A+" },
            { acronym: "SIGMETRICS", name: "ACM SIG on computer and communications metrics and performance", rank: "A+" },
            { acronym: "SIGMOD", name: "ACM Special Interest Group on Management of Data Conference", rank: "A+" },
            { acronym: "SODA", name: "ACM/SIAM Symposium on Discrete Algorithms", rank: "A+" },
            { acronym: "SOSP", name: "ACM SIGOPS Symposium on Operating Systems Principles", rank: "A+" },
            { acronym: "STOC", name: "ACM Symposium on Theory of Computing", rank: "A+" },
            { acronym: "UAI", name: "Conference in Uncertainty in Artificial Intelligence", rank: "A+" },
            { acronym: "UbiComp", name: "Ubiquitous Computing", rank: "A+" },
            { acronym: "VLDB", name: "International Conference on Very Large Databases", rank: "A+" },
            { acronym: "WWW", name: "International World Wide Web Conference", rank: "A+" },
			
        ];

        const conferencesPerPage = 30;
        let currentPage = 1;

        const searchInput = document.getElementById('search');
        const rankFilter = document.getElementById('rankFilter');
        const conferenceTableBody = document.getElementById('conferenceTableBody');
        const prevPageButton = document.getElementById('prevPage');
        const nextPageButton = document.getElementById('nextPage');

        const renderTable = () => {
            const searchValue = searchInput.value.toLowerCase();
            const rankValue = rankFilter.value;

            const filteredConferences = conferences.filter((conference) => {
                const matchesSearch = conference.name.toLowerCase().includes(searchValue) || conference.acronym.toLowerCase().includes(searchValue);
                const matchesRank = rankValue === 'all' || conference.rank === rankValue;
                return matchesSearch && matchesRank;
            });

            const startIndex = (currentPage - 1) * conferencesPerPage;
            const endIndex = Math.min(startIndex + conferencesPerPage, filteredConferences.length);
            const paginatedConferences = filteredConferences.slice(startIndex, endIndex);

            conferenceTableBody.innerHTML = '';
            paginatedConferences.forEach((conference, index) => {
                const row = document.createElement('tr');
                row.innerHTML = `
                    <td>${startIndex + index + 1}</td>
                    <td>${conference.acronym}</td>
                    <td>${conference.name}</td>
                    <td>${conference.rank}</td>
                `;
                conferenceTableBody.appendChild(row);
            });

            prevPageButton.classList.toggle('disabled', currentPage === 1);
            nextPageButton.classList.toggle('disabled', currentPage * conferencesPerPage >= filteredConferences.length);
        };

        searchInput.addEventListener('input', () => {
            currentPage = 1;
            renderTable();
        });

        rankFilter.addEventListener('change', () => {
            currentPage = 1;
            renderTable();
        });

        prevPageButton.addEventListener('click', () => {
            if (currentPage > 1) {
                currentPage--;
                renderTable();
            }
        });

        nextPageButton.addEventListener('click', () => {
            currentPage++;
            renderTable();
        });

        renderTable();
    </script>
